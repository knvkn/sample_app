<% provide(:title, 'Мої данні') %><h1>Антропологічні дані</h1><div class="datet">Сьогодні: <%= Time.now.strftime("%m/%d/%Y") %></div><div class="map_pn"><p>Дані за період: <%= @day.strftime("%m/%d/%Y") %></p><div class="back_day"><%#= link_to "попередній день", :date => @day - 1 %></div><div class="next_day"><%#= link_to "наступний день", :date => @day + 1 %></div></div><div class="map_pn">	<div class="back_tr">		<% if @previous_item %>			<%= link_to "попередні заміри", :date => @previous_item.day %>		<% end %>	</div>	<div class="next_tr">		<% if @next_item %>			<%= link_to "наступна заміри", :date => @next_item.day %>		<% end %>	</div></div><div class="body_wrap">		<div class="datas">		<% @message.each do |t| %>				<div class="inddata neck"><%= t.neck %><span>см</span></div>				<div class="inddata pecs"><%= t.pecs %><span>см</span></div>				<div class="inddata arms"><%= t.arms %><span>см</span></div>				<div class="inddata waistline"><%= t.waistline %><span>см</span></div>				<div class="inddata thigh"><%= t.thigh %><span>см</span></div>				<div class="inddata triceps_surae"><%= t.triceps_surae %><span>см</span></div>				<div class="inddata weight"><%= t.weight %><span>кг</span></div>		<% end %>		</div>	<div class="bodyhuman">		<div class="newdata">		<%= form_for(@anthropological, :url => {:action => "create"}) do |f| %>				  <div class="minf neck"><%= f.number_field :neck, min: 0, placeholder: "0" %>см</div>		  <div class="minf pecs"><%= f.number_field :pecs, min: 0, placeholder: "0" %>см</div>		  <div class="minf arms"><%= f.number_field :arms, min: 0, placeholder: "0" %>см</div>		  <div class="minf waistline">см<%= f.number_field :waistline, min: 0, placeholder: "0" %></div>		  <div class="minf thigh">см<%= f.number_field :thigh, min: 0, placeholder: "0" %></div>		  <div class="minf triceps_surae">см<%= f.number_field :triceps_surae, min: 0, placeholder: "0" %></div>		  <div class="minf weight"><%= f.number_field :weight, min: 0 %>кг</div>	            <%= f.submit "Додати", class: "btn btn-large btn-primary boddybutt" %>		<% end %>		</div>	</div></div>